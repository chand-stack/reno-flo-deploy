{
    "version": 2,
    "builds": [
      {
        "src": "reno-flo-landing.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.{jpg,jpeg,png,gif,webp,svg,ico}",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(jpg|jpeg|png|gif|webp|svg|ico))",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/reno-flo-landing.html"
      }
    ]
  }
  